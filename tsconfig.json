{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "types": ["@prisma/client"],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": "./",
    "paths": {
      "@/*": ["./*"],
      "@components/*": ["./app/components/*"],
      "@styles/*": ["./app/styles/*"],
      "@services/*": ["./app/services/*"],
      "@utils/*": ["./app/utils/*"],
      "@pipe/*": ["./app/pipe/*"],
      "@interface/*": ["./app/interface/*"],
      "@provider/*": ["./app/provider/*"],
      "@style/*": ["./app/style/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/components/auth/Logged"],
  "exclude": ["node_modules"]
}
